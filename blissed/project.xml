      <unsubscribe>http://lists.codehaus.org/mailman/listinfo/blissed-cvs</unsubscribe>
      <archive>http://lists.codehaus.org/pipermail/blissed-cvs/</archive>
    </mailingList>
  </mailingLists>

  <siteAddress>blissed.codehaus.org</siteAddress>
  <siteDirectory>/www/blissed.codehaus.org</siteDirectory>
  <issueTrackingUrl>http://jira.codehaus.org/secure/BrowseProject.jspa?id=10011&amp;report=roadmap</issueTrackingUrl>

  <repository>
    <connection>scm:cvs:pserver:anonymous@cvs.codehaus.org:/cvsroot/blissed:blissed</connection>
