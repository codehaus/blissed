    <developer>
      <name>peter royal</name>
      <id>proyal</id>
      <roles>
        <role>Developer</role>
      </roles>
      <email>proyal@pace2020.com</email>
      <organization>Pace Systems Group</organization>
    </developer>
  </developers>

  <dependencies>

    <dependency>
      <id>commons-beanutils</id>
      <version>1.4-dev</version>
    </dependency>

    <dependency>
      <id>commons-collections</id>
      <version>2.0</version>
    </dependency>

    <dependency>
      <id>commons-graph</id>
      <version>0.8</version>
    </dependency>

    <dependency>
      <id>commons-jelly</id>
      <version>SNAPSHOT</version>
      <url>http://jakarta.apache.org/commons/jelly/</url>
    </dependency>

    <dependency>
      <groupId>commons-jelly</groupId>
      <artifactId>commons-jelly-tags-junit</artifactId>
      <version>SNAPSHOT</version>
      <url>http://jakarta.apache.org/commons/jelly/</url>
    </dependency>

    <dependency>
      <id>commons-logging</id>
      <version>1.1-dev</version>
      <url>http://jakarta.apache.org/commons/logging/</url>
    </dependency>

    <dependency>
      <id>dom4j</id>
      <version>1.4-dev-3</version>
      <url>http://dom4j.org/</url>
    </dependency>

    <dependency>
      <id>commons-jexl</id>
      <version>1.0-beta-1</version>
      <url>http://jakarta.apache.org/commons/sandbox/jexl/</url>
    </dependency>

    <dependency>
      <id>junit</id>
      <version>3.7</version>
      <url>http://junit.org/</url>
    </dependency>

    <dependency>
      <id>xerces</id>
      <version>2.0.0</version>
    </dependency>

    <dependency>
      <id>xml-apis</id>
      <version>2.0.0</version>
      <url>http://xml.apache.org/xerces2-j/</url>
    </dependency>
  </dependencies>

  <build>
    <nagEmailAddress>proyal@pace2020.com</nagEmailAddress>
    <sourceDirectory>src/java/main</sourceDirectory>
    <unitTestSourceDirectory>src/java/test</unitTestSourceDirectory>
    <aspectSourceDirectory/>

    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
      <resources>
        <resource>
          <directory>src/java/test</directory>
          <includes>
            <include>**/*.jelly</include>
          </includes>
        </resource>
      </resources>
    </unitTest>

    <resources>
      <includes>
        <include>log4j.properties</include>
      </includes>
    </resources>

    <jars/>
  </build>
</project>
